{
  "settings": {
    "react": {
      "pragma": "m"
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "extends": [
    "eslint-config-standard",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/no-deprecated": 0,
    "react/display-name": 0
  }
}
